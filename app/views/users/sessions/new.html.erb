

<h2>Twitterにログイン</h2>

<div class="container">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :電話、メールアドレスまたはユーザー名, class: "t" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"f" %>
  </div>

  <div class="field">
    <%= f.label :パスワード, class: "t" %><br />
    <%= f.password_field :password, autocomplete: "current-password",class:"f" %>
  </div>
  
  
  <div class="actions">
    <%= f.submit "ログイン",class: "login" %>
  </div>
<% end %>

<%= render "users/shared/links" %>

</div>
